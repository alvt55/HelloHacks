{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "images/hello_extensions.png",
    "default_title": "Click to open panel"
  },
  "permissions": [
    "contextMenus",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "backend/sidepanel.html"
  },
  "background": {
    "service_worker": "backend/service-worker.js"
  }
}
